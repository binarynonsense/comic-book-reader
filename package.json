{
  "name": "acbr-comic-book-reader",
  "version": "1.2.4",
  "description": "ACBR - A comic book reader and converter for cbz and cbr files",
  "main": "./app/main.js",
  "author": "Álvaro García (www.binarynonsense.com)",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/binarynonsense/comic-book-reader"
  },
  "build": {
    "appId": "com.binarynonsense.cbreader",
    "linux": {
      "category": "Utility",
      "icon": "./app/assets/images/icon_256x256.png"
    },
    "win": {
      "icon": "./app/assets/images/icon_256x256.png"
    }
  },
  "scripts": {
    "start": "electron . --dev",
    "build:linux": "electron-builder --linux --dir",
    "dist:linux": "electron-builder --linux",
    "build:windows": "electron-builder --win --dir",
    "dist:windows": "electron-builder --win"
  },
  "devDependencies": {
    "electron": "^9.3.0",
    "electron-builder": "^22.6.1",
    "license-checker": "^25.0.1"
  },
  "dependencies": {
    "adm-zip": "^0.4.14",
    "custom-electron-titlebar": "^3.2.4",
    "epub": "^1.2.1",
    "epub-gen": "^0.1.0",
    "file-type": "^14.6.2",
    "jimp": "^0.13.0",
    "natural-compare-lite": "^1.4.0",
    "node-unrar-js": "^0.8.1",
    "pdfkit": "^0.11.0"
  }
}
